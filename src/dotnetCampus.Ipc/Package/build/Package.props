<Project>

  <PropertyGroup>
    <!--
      The analyzers in this package is very important, so we treat related warnings as errors.
      - CS8785: Analyzer does not generate sources
      - CS9057: Analyer version is greator than the compiler version
    -->
    <WarningsAsErrors>$(WarningAsErrors);CS8785;CS9057</WarningsAsErrors>
  </PropertyGroup>

</Project>
